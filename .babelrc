{
  "presets": [
    "@babel/preset-react",
    "@babel/preset-flow",
    [
      "@babel/preset-env",
      {
        "corejs": {
          "version": "2",
          "proposals": true
        },
        "useBuiltIns": "usage"
      }
    ]
  ],
  "plugins": [
    "lodash",
    "styled-components",
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    "@babel/plugin-transform-destructuring",
    "@babel/plugin-transform-regenerator",
    "transform-class-properties"
  ]
}