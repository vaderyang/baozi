{
  "verbose": false,
  "rootDir": "..",
  "roots": [
    "<rootDir>/server",
    "<rootDir>/shared"
  ],
  "moduleNameMapper": {
    "^@server/(.*)$": "<rootDir>/server/$1",
    "^@shared/(.*)$": "<rootDir>/shared/$1"
  },
  "setupFiles": [
    "<rootDir>/__mocks__/console.js",
    "./server/test/setup.ts"
  ],
  "testEnvironment": "node"
}